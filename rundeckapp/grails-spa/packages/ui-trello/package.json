    "moment": "^2.25.3",
    "perfect-scrollbar": "^1.5.0",
    "prex": "^0.4.7",
    "uiv": "^0.33.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.22.4",
    "vue-multiselect": "^2.1.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.3.2",
    "vue-virtual-scroller": "^1.0.10",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/vue": "^6.2.9",
