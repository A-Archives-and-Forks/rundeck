    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
    "concurrently": "^7.0.0",
    "fetch-mock": "^9.10.1",
    "mobx-utils": "^5.6.1",
    "mobx-vue": "^2.0.11",
    "run-script-os": "^1.1.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "uuid": "^8.3.2",
    "vue-cli-plugin-tsx-support": "~0.4.1",
    "vue-jsx-hot-loader": "^1.4.1",
    "vue-template-compiler": "^2.6.11",
    "vue-tsx-support": "^3.2.0",
    "walk": "^2.3.15"
